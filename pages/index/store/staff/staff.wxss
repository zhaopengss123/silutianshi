/* pages/index/store/staff/staff.wxss */
.memberList{
  width: 680rpx;
  margin: 0 auto;
  overflow: hidden;
  background: #fff;
  padding: 0 35rpx;
}
.memberList>view{
  padding: 40rpx 0 ;
  border-bottom: solid 1px #f0f0f0;
  overflow: hidden;
}
.member_left{
  float: left;
}
.member_right{
  float: right;
}
.member_name{
  font-size: 28rpx;
  color: #373C41;
}
.member_tel{
  font-size: 20rpx;
  color: #919BA5;
}
.warning{
  font-size: 24rpx;
  width: 680rpx;
  margin: 0 auto;
  color: #FD704F;
  margin-top: 28rpx;
}
.addmember{
  width: 100%;
  height:110rpx;
  line-height: 110rpx;
  position: fixed;
  bottom: 0;
  text-align: center;
  background: #FFC41F;
  color: #373C41;
  font-size: 32rpx;
}