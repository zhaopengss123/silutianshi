.linemune{
  width: 680rpx;
  margin: 0 auto;
}
.linemune view{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  border-bottom: solid 1px #ccc;
}
.storeListmnue view:first-child{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
}
.storeListmnue view:last-child{
  height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  margin-left: 30rpx;
  float: left;
}
.storeListmnue{
  width: 680rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.shopName{
  clear: both;
  font-size: 30rpx;
  width: 680rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.showBtn{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0, 0, 0, .5);
}
.showBtnMain{
  width: 650rpx;  
  height: 400rpx;
  position: fixed;
  top: 50%;
  margin-top: -200rpx;
  margin-left: 50rpx;
  background:#fff;
  border: solid 1rpx #ccc;
  border-radius: 5rpx;
}
.showBtnMain text{
  display: block;
  margin: 0 auto;
  font-size: 30rpx;
  text-align: center;
  color: #666;
  margin-top: 110rpx;
}
.showBtnMain button{
  width: 350rpx;
  margin-top: 50rpx;
  background: rgb(64, 158, 255);
  color: #fff;
}
.activity{
  width: 670rpx;
  margin: 0 auto;
  overflow: hidden;
}
.activityTitle{
  height: 120rpx;
  line-height: 120rpx;
  font-weight: bold;
}
.activitylist{
  overflow: hidden;
}
.activitylist>view{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: solid 2rpx #ccc;
}
.topback{
  width: 200rpx;
  height: 200rpx;
  position: fixed;
  top: -100rpx;
  left: 0;
  background: #ccc;
  z-index: 999999;
}