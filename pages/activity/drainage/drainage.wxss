}

.textareas text {
  position: absolute;
  bottom: 30rpx;
  right: 40rpx;
  color: #c3cdd7;
  line-height: normal;
}

.selectDate picker {
  width: auto;
  float: right;
}

.selectDate picker view {
  color: #919ba5;
}

