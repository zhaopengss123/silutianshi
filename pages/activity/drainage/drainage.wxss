/* pages/index/store/store.wxss */
.content{
  padding-bottom: 120rpx;
}
.store_img {
  width: 670rpx;
  height: 338rpx;
  position: relative;
  margin: 0 auto;
  box-shadow: 0rpx 0rpx 5rpx #ccc;
  border-radius: 30rpx;
  overflow: hidden;
}

.store_img>view {
  width: 748rpx;
  height: 290rpx;
  border: dashed 1rpx #ccc;
  line-height: 290rpx;
  text-align: center;
  color: #ccc;
  font-size: 35rpx;
  border-radius: 30rpx;
}

.store_img>image {
  width: 670rpx;
  height: 350rpx;
}

.store_img_btn.store_img_btn {
  width: 176rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  background: rgba(255, 196, 31, 1);
  border-radius: 58rpx;
  color: #fff;
  font-size: 28rpx;
  position: absolute;
  top: 212rpx;
  left: 236rpx;
}

.store_img text {
  display: block;
  width: 750rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  background: rgba(0, 0, 0, 0.5);
  color: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  font-size: 25rpx;
}

.mnue-list {
  width: 670rpx;
  margin: 0 auto;
  padding: 0 35rpx;
  background: #fff;
}

.mnue-list>view {
  height: 108rpx;
  line-height: 108rpx;
  font-size: 28rpx;
  color: #222;
}

.mnue-list>view view{
  float: left;
}

.mnue-list>view view:nth-child(2) {
  float: right;
}

.openvip {
  width: 650rpx;
  border: solid 1rpx #ccc;
  border-radius: 15rpx;
  position: absolute;
  top: 220rpx;
  left: 50rpx;
  height: 120rpx;
  background: #fff;
  padding: 10rpx;
}

.vip_left {
  float: left;
  margin-left: 20rpx;
}

.vip_left view {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}

.vip_left view:last-child {
  color: #999;
  font-size: 25rpx;
}

.vip_right {
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: solid 1px #ccc;
  float: right;
  border-radius: 10rpx;
  text-align: center;
  margin-top: 30rpx;
  margin-right: 20rpx;
  font-size: 12px;
}

.mnue-list input, picker {
  width: 450rpx;
  height: 108rpx;
  font-size: 27rpx;
  text-align: right;
}

picker view {
  width: 100%;
}

.uploadcode.uploadcode {
  width: 680rpx;
  margin: 0 auto;
  min-height: 400rpx;
  line-height: normal;
  clear: both;
  border-bottom: solid 2rpx #eff0f4;
  height: auto;
  overflow: hidden;
}

.uploadleft {
  font-size: 27rpx;
  margin-top: 70rpx;
  float: left;
}
.uploadleft>view{
  clear: both;
  line-height: normal;
}
.uploadleft>view:last-child {
  font-size: 20rpx;
  margin-top: 5rpx;
  color: #919ba5;
}



.add_icon {
  width: 100%;
  text-align: center;
  font-size: 100rpx;
  color: #999;
}

.add_text {
  width: 100%;
  text-align: center;
  font-size: 20rpx;
  color: #999;
}

.right-icon {
  background: url(https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/store/icon-right-o.png) no-repeat right center;
  background-size: 15.2rpx 27.2rpx;
  padding-right: 35rpx;
  font-size: 32rpx;
}

.right-icon view:nth-child(2) {
  color: #919ba5;
  font-size: 24rpx;
}

.boldfont {
  font-weight: bold;
  border-bottom: solid 2rpx #eff0f4;
}

.toggle-icon {
  width: 670rpx;
  height: 158rpx;
  font-size: 32rpx;
  margin: 0 35rpx;
  color: #222;
  font-weight: 500;
}

.toggle-icon view {
  float: left;
  margin-top: 40rpx;
}

.toggle-icon text {
  color: #919ba5;
  font-size: 20rpx;
  display: block;
  margin-top: 6rpx;
}

.toggle-icon view:last-child {
  float: right;
}

.warning {
  font-size: 25rpx;
  color: red;
}

.noborder.noborder {
  border: none;
}

textarea {
  width: 100%;
  height: 150rpx;
  line-height: 30rpx;
}

.textareas.textareas {
  border: solid 1px #f0f0f0;
  padding: 20rpx 15rpx;
