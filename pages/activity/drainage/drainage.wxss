}

.toggle-icon-text {
  width: 680rpx;
  height: 120rpx;
  line-height: 45rpx;
  font-size: 28rpx;
  padding-bottom: 20rpx;
  margin-top: 10rpx;
  border-bottom: solid 2rpx #eff0f4;
  margin: 0 auto;
}

.toggle-icon-text view {
  float: left;
}

.toggle-icon-text view:last-child {
  float: right;
  margin-top: 20rpx;
}

.toggle-icon-text view text {
